
{
  "version": 2,
  "installCommand": "cd client && HUSKY_SKIP_INSTALL=1 yarn install",
  "builds": [
    {
      "src": "package.json",       
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
